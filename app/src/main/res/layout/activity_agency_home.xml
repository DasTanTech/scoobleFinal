<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".agencyHomeActivity">

    <FrameLayout
        android:id="@+id/agencyHome_frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/agencyHome_BNV"/>


    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:id="@+id/agencyHome_BNV"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:mbn_circleColor="@color/colorWhite"
        app:mbn_backgroundBottomColor="@color/colorWhite"
        app:mbn_selectedIconColor="@color/colorPrimary"
        app:mbn_rippleColor="@color/colorWhite"/>

</RelativeLayout>